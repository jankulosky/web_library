elete={this.deleteBook}\r\n                            onEdit={this.getBook}/>}/>\r\n            </div>\r\n          </main>\r\n        </Router>\r\n    );\r\n  }\r\n\r\n  componentDidMount() {\r\n    this.loadAuthors();\r\n    this.loadCategories();\r\n    this.loadBooks();\r\n  }\r\n\r\n  loadAuthors = () => {\r\n    ELibraryService.fetchAuthors()\r\n        .then((data) => {\r\n          this.setState({\r\n            authors: data.data\r\n          })\r\n        });\r\n  }\r\n\r\n  loadBooks = () => {\r\n    ELibraryService.fetchBooks()\r\n        .then((data) => {\r\n          this.setState({\r\n            books: data.data\r\n          })\r\n        });\r\n  }\r\n\r\n  loadCategories = () => {\r\n    ELibraryService.fetchCategories()\r\n        .then((data) => {\r\n          this.setState({\r\n            categories: data.data\r\n          })\r\n        });\r\n  }\r\n\r\n  deleteBook = (id) => {\r\n    ELibraryService.deleteBook(id)\r\n        .then(() => {\r\n          this.loadBooks();\r\n        });\r\n  }\r\n\r\n  addBook = (id, name, category, author, availableCopies) => {\r\n    ELibraryService.addBook(id, name, category, author, availableCopies)\r\n        .then(() => {\r\n          this.loadBooks();\r\n        });\r\n  }\r\n\r\n  getBook = (id) => {\r\n    ELibraryService.getBook(id)\r\n        .then((data) => {\r\n          this.setState({\r\n            selectedBook: data.data\r\n          })\r\n        })\r\n  }\r\n\r\n  editProduct = (id, name, category, author, availableCopies) => {\r\n    ELibraryService.editBook(id, name, category, author, availableCopies)\r\n        .then(() => {\r\n          this.loadBooks();\r\n        });\r\n  }\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\vikto\\Downloads\\web-library\\frontend\\src\\components\\Authors\\authors.js",[],"C:\\Users\\vikto\\Downloads\\web-library\\frontend\\src\\components\\Categories\\categories.js",[],"C:\\Users\\vikto\\Downloads\\web-library\\frontend\\src\\repository\\elibraryRepositor